pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Jenkins Build the program'
            }
        }
        
        stage('Deploy') {
            steps {
                echo 'Jenkins Deploy the program'
            }
        }
            
        stage('Test') {
            steps {
                echo 'Jenkins Test the program'
            }
        }
    }
}
